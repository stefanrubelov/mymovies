<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mymovies.gui.Homepage">
   <children>
      <TableView fx:id="movieList" layoutX="25.0" layoutY="48.0" prefHeight="261.0" prefWidth="551.0">
