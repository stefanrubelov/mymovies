      <Label layoutX="485.0" layoutY="323.0" text="Category">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <Button fx:id="addCategoryBtn" layoutX="445.0" layoutY="350.0" mnemonicParsing="false" prefWidth="62.0" text="Add" />
      <Button fx:id="removeCategoryBtn" layoutX="514.0" layoutY="350.0" mnemonicParsing="false" text="Remove" />
      <Label layoutX="236.0" layoutY="341.0" text="Currently playing..">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
