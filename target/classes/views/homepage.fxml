          <TableColumn fx:id="movieColumn" prefWidth="110.0" text="Movie" />
          <TableColumn fx:id="categoryColumn" prefWidth="110.0" text="Category" />
            <TableColumn fx:id="ratingColumn" prefWidth="110.0" text="Rating" />
            <TableColumn fx:id="linkColumn" prefWidth="110.0" text="File link" />
            <TableColumn fx:id="lastViewColumn" prefWidth="110.0" text="Last View" />
        </columns>
      </TableView>
      <Label layoutX="225.0" layoutY="14.0" text="My movie collection">
         <font>
            <Font name="System Bold" size="19.0" />
         </font>
      </Label>
      <Label layoutX="71.0" layoutY="322.0" text="Movies">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Label>
      <Button fx:id="addMovieBtn" layoutX="25.0" layoutY="349.0" mnemonicParsing="false" prefWidth="62.0" text="Add" />
